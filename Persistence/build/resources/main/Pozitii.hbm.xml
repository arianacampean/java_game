<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
        <class name="domeniu.Pozitii"
               table="Pozitii">
            <id name="id" column="id_poz" type="integer">
                <generator class="increment"/>
            </id>
            <property
                    name="id_juc"
                    column="id_juc"
                    type="int"/>

            <property
                    name="pozitie"
                    column="pozitie"
                    type="int"/>


        </class>
</hibernate-mapping>

